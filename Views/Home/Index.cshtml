@{
    ViewData["Title"] = "Home Page";
}





<div id="services" class="services_section layout_padding">
    <div class="container">
        <h1 class="services_taital text-center">Our Services</h1>
        <p class="many_taital text-center">Providing essential services to support our community's well-being.</p>
        <div class="services_section2 layout_padding">
            <div class="row justify-content-center">
                <div class="col-lg-3 col-sm-6 text-center d-flex">
                    <div class="card w-100">
                        <div class="card-body">
                            <div class="icon_1">
                                <img src="images/FoodPacks.png" alt="Food Packs" class="img-fluid services-img">
                            </div>
                            <h5 class="card-title furnitures_text">Food Packs</h5>
                            <p class="card-text dummy_text">
                                We are honored to distribute these food packs as part of our commitment to supporting our community. May this help ease your burdens and bring nourishment to your home.
                            </p>
                            <a asp-controller="FoodPackForms" asp-action="Create" class="btn btn-info">Request Now!</a>

                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-sm-6 text-center d-flex">
                    <div class="card w-100">
                        <div class="card-body">
                            <div class="icon_1">
                                <img src="images/Consultation.png" alt="Consultation" class="img-fluid services-img">
                            </div>
                            <h5 class="card-title furnitures_text">Consultation</h5>
                            <p class="card-text dummy_text">Consultation for Solo Parents!</p>
                            <a asp-controller="Forms" asp-action="ConsultationForms" class="btn btn-info">Schedule Now!</a>
                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-sm-6 text-center d-flex">
                    <div class="card w-100">
                        <div class="card-body">
                            <div class="icon_1">
                                <img src="images/certificate.png" alt="Certificate of Indigency" class="img-fluid services-img">
                            </div>
                            <h5 class="card-title furnitures_text">Certificate of Indigency</h5>
                            <p class="card-text dummy_text">The certificate serves as proof that the bearer qualifies for financial aid or benefits based on their economic situation.</p>
                            <a asp-controller="Forms" asp-action="CertificateOfIndigencyForm" class="btn btn-info">Get Now!</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


